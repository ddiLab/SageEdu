### IP Camera Testing

import numpy as np
import cv2
FrameWidth=480
FrameHeight=480
cap = cv2.VideoCapture()
cap.open("rtsp://admin:Sledge!44@10.0.0.153:554/cam/realmonitor?channel=1&subtype=0")

while (True):
    #Capture frame-by-fram
    ret, frame = cap.read()

    cv2.imshow('frame', frame)

    if cv2.waitKey(1) & 0xFF == ord('q'):
        break

